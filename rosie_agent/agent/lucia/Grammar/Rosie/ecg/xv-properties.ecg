//  Miscellaneous adjectives
construction CURRENT
   subcase of Property
   form
      constraints
         self.f.orth <-- "current"
   meaning: PropertyDescriptor
      constraints
         self.m.class <-- @state
         self.m.name <-- "current"

construction EMPTY
   subcase of Property
   form
      constraints
         self.f.orth <-- "empty"
   meaning: PropertyDescriptor
      constraints
         self.m.class <-- @state
         self.m.name <-- "empty"

construction LIT
   subcase of Property
   form
      constraints
         self.f.orth <-- "lit"
   meaning: PropertyDescriptor
      constraints
         self.m.class <-- @state
         self.m.name <-- "on2"

