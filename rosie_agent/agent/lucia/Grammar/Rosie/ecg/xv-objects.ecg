// Spatial shapes
schema Room
   subcase of SpatialShape
   constraints
      rosie-category <-- spatial-shape
      spatial-shape <-- room1

// Persons
schema Rosie
   subcase of RosieObject
   roles
      name
   constraints
      rosie-category <-- person
      movable <-- false
      name <-- rosie1
      handle <-- rosie

schema Bob
   subcase of KnownObject
   roles
      name
   constraints
      rosie-category <-- person
      movable <-- false
      name <-- Bob
      handle <-- bob

// Objects
schema Lights
   subcase of KnownObject
   roles
      name
   constraints
      rosie-category <-- object
      movable <-- false
      name <-- lights1
      handle <-- conference-room-lights1

schema Steps
   subcase of KnownObject
//   roles
//      name
   constraints
      rosie-category <-- object
      movable <-- false
      handle <-- steps1

//  Concepts
schema Question
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- question

schema Answer
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- answer1

schema Response
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- response

schema Message
   subcase of Concept
   constraints
      rosie-category <-- concept
      concept-handle <-- message
