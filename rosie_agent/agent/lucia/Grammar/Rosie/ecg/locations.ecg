//	The following constructions were in the separate Rosie grammar
schema RosieLocation
   roles
     rosie-category
	 name

schema Stove
   subcase of RosieLocation
   constraints
      rosie-category <-- location
	  name <-- stove

schema Pantry
   subcase of RosieLocation
   constraints
      rosie-category <-- location
	  name <-- pantry

schema Garbage
   subcase of RosieLocation
   constraints
      rosie-category <-- location
	  name <-- garbage

schema Table
   subcase of RosieLocation
   constraints
      rosie-category <-- location
      name <-- table
