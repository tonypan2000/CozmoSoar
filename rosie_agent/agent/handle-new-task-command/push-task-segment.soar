
sp {handle-new-task-command*propose*push-task-segment
   (state <s> ^name handle-new-task-command
              ^new-task-segment <seg>
             -^pushed-task-segment <seg>)
-->
   (<s> ^operator <o> + >)
   (<o> ^name push-task-segment
        ^segment <seg>)
}

sp {handle-new-task-command*apply*push-task-segment*mark*pushed-task-segment
   (state <s> ^name handle-new-task-command
              ^operator <o>
              ^new-task-segment <seg>)
   (<o> ^name push-task-segment 
        ^segment <seg>
        ^status complete)
-->
   (<s> ^pushed-task-segment <seg>)
}

