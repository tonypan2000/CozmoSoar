sp {op_deliver1*elaborate*smem-query*corner
   (state <s> ^name op_deliver1
              ^corner-location.handle <handle>
             -^corner-location-info)
-->
   (<s> ^smem-query <query>)
   (<query> ^cue <cue>
            ^depth 2
            ^destination <s>
            ^result-name corner-location-info)
   (<cue> ^handle <handle>)
}
