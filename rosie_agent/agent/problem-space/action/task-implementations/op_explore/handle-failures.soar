# unsatisifed-goal: the explore command finished without the until clause being satisfied
sp {op_explore1*propose*handle-execution-failure*unsatisifed-goal*after*explore
   (state <s> ^name op_explore1
              ^current-task-segment <seg>
              ^until-clause <until>
              ^unvisited-waypoints false
              ^top-state.world.robot.current-waypoint.handle <wp-handle>)
   (<until> -^satisfied true)
   (<seg> ^scanned <wp-handle>)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name handle-execution-failure
        ^subtype unsatisfied-goal
        ^category failure-handling
        ^task-segment <seg>)
}
