sp {op_explore1*prefer*scan*over*go-to
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +
             -^child-task-segment)
   (<o1> ^name op_scan1)
   (<o2> ^name op_go-to-waypoint1)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {op_explore1*prefer*go-to*shorter*distance
   (state <s> ^name op_explore1
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name op_go-to-waypoint1
         ^distance <d>)
   (<o2> ^name op_go-to-waypoint1
         ^distance > <d>)
-->
   (<s> ^operator <o1> > <o2>)
}
