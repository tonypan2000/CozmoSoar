sp {op_give1*magicbot*propose*op_approach
   (state <s> ^name op_give1
              ^problem-space.execution-type external
              ^desired-distance <dist>
              ^distance-to-person > <dist>
              ^person <person>)
-->
   (<s> ^operator <o> +)
   (<o> ^name op_approach1
        ^item-type task-operator
        ^task-handle approach1
        ^arg1 <arg1>)
   (<arg1> ^arg-type object
           ^id <person>)
}
