sp {op_give*apply*complete-task*add*holding*predicate
   (state <s> ^name op_give1
              ^operator.name complete-task
              ^held-object <obj>
              ^person <person>
              ^top-state.world.predicates.predicate <in-pred>)
   (<in-pred> ^handle holding1)
   -{(<in-pred> ^instance <cur-i>)
     (<cur-i> ^1 <obj> ^2 <person>)}
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <person> ^2 <obj>)
}

sp {op_give*apply*complete-task*add*in*predicate
   (state <s> ^name op_give1
              ^operator.name complete-task
              ^held-object <obj>
              ^top-state.world.predicates.predicate <in-pred>
              ^top-state.world.robot.current-location <loc>)
   (<in-pred> ^handle in1)
   -{(<in-pred> ^instance <cur-i>)
     (<cur-i> ^1 <obj> ^2 <loc>)}
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
}
