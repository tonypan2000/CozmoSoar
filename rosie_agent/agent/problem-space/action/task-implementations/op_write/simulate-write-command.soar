sp {op_write1*propose*simulate-write-command
   (state <s> ^name op_write1
              ^problem-space.execution-type external
              ^current-task-segment <seg>)
   (<seg> ^write-command <cmd>)
   (<cmd> ^name write
         -^status complete)
-->
   (<s> ^operator <o> +)
   (<o> ^name simulate-write-command
        ^item-type command-operator
        ^command <cmd>)
}

sp {op_write1*apply*simulate-write-command*mark*command*complete
   (state <s> ^name op_write1
              ^operator <o>)
   (<o> ^name simulate-write-command
        ^command <cmd>)
-->
   (<cmd> ^status complete)
}

sp {op_write1*apply*simulate-write-command*add*value
   (state <s> ^name op_write1
              ^operator.name simulate-write-command
              ^target-object.predicates <preds>
              ^target-value <val>)
-->
   (<preds> ^value <val>)
}
