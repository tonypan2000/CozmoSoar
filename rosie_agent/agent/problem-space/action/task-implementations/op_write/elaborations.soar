}

sp {op_write1*elaborate*target-value*superoperator
   (state <s> ^name op_write1
              ^task-operator.arg1 <a1>)
   (<a1> ^handle <num>)
-->
   (<s> ^target-value <num>)
}

#### GOAL SATISFACITON ####
sp {op_write1*elaborate*desired
   (state <s> ^name op_write1
             -^copy-task-goal
              ^target-value <val>
              ^target-object <obj>)
-->
   (<s> ^desired <des>)
   (<des> ^pred-count 1 ^1 <p1>)
   (<p1> ^type unary ^handle <val> ^property-handle value ^1 <obj>)
}

