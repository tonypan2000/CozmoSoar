sp {op_recall1*propose*create-epmem-cue
  (state <s> ^name op_recall1
            -^epmem-cue)
-->
  (<s> ^operator <o> + =)
  (<o> ^name create-epmem-cue)
}

sp {op_recall1*apply*create-epmem-cue*create*base*cue
  (state <s> ^name op_recall1
             ^operator.name create-epmem-cue)
-->
  (<s> ^epmem-cue <cue>)
  (<cue> ^world <w>)
  (<w> ^objects <objs>
       ^predicates <preds>
       ^robot <rob>)
}


