sp {op_go-to-next-waypoint1*propose*advance-position*drive-xy
   (state <s> ^name op_go-to-next-waypoint1
              ^problem-space.execution-type external
              ^current-task-segment.drive-command.status success
              ^current-task-segment.position-set.next.next { <next> <> none })
-->
   (<s> ^operator <o> +)
   (<o> ^name advance-position)
}

sp {op_go-to-next-waypoint1*apply*advance-position*change*next*position
   (state <s> ^name op_go-to-next-waypoint1
              ^operator.name advance-position
              ^current-task-segment.position-set <set>)
   (<set> ^next <cur>)
   (<cur> ^next <next>)
-->
   (<set> ^next <cur> -
          ^next <next>)
}

sp {op_go-to-next-waypoint1*apply*advance-position*cleanup*command
   (state <s> ^name op_go-to-next-waypoint1
              ^operator.name advance-position
              ^io.output-link <out>
              ^current-task-segment <seg>)
   (<seg> ^{ <cmd-type> << face-command drive-command >> } <cmd>)
   (<out> ^<cmd-name> <cmd>)
-->
   (<seg> ^<cmd-type> <cmd> -)
   (<out> ^<cmd-name> <cmd> -)
}
 
