
sp {primitive-pick-up*ai2thor*propose*send-pick-up-command
   (state <s> ^name primitive-pick-up
              ^agent-params.domain ai2thor
              ^problem-space.execution-type external
              -^current-task-segment.pick-up-command
              ^target-object <obj>)
-->
   (<s> ^operator <o> +)
   (<o> ^name send-pick-up-command
        ^object <obj>)
}

sp {primitive-pick-up*ai2thor*apply*send-pick-up-command*send
   (state <s> ^name primitive-pick-up
              ^agent-params.domain ai2thor
              ^operator <o>
              ^io.output-link <ol>
              ^current-task-segment <seg>)
   (<o> ^name send-pick-up-command
        ^object.handle <h>)
-->
   (<ol> ^perform-action <pa>)
   (<pa> ^name pick-up
         ^object <h>)
   (<seg> ^pick-up-command <pa>)
}
