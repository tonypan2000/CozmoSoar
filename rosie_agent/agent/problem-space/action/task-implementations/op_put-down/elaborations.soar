}

sp {op_put-down1*cozmo*propose*add-in-predicate
   (state <s> ^name op_put-down1
              ^agent-params.domain cozmo
              ^current-task-segment.put-down-command.status succeeded
              ^held-object <obj>
              ^world.objects.object <loc>
              ^world.predicates.predicate <in-pred>)
   (<loc> ^predicates.modifier1 current1)
   (<in-pred> ^handle in1)
   -{(<in-pred> ^instance <i>)
     (<i> ^1 <obj> ^2 <loc>)}
-->
   (<s> ^operator <o> + >)
   (<o> ^name add-in-predicate)
}

sp {op_put-down*cozmo*apply*add-in-predicate
   (state <s> ^name op_put-down1
              ^operator.name add-in-predicate
              ^held-object <obj>
              ^world.objects.object <loc>
              ^world.predicates.predicate <in-pred>)
   (<loc> ^predicates.modifier1 current1)
   (<in-pred> ^handle in1)
-->
   (<in-pred> ^instance <i>)
   (<i> ^1 <obj> ^2 <loc>)
}


