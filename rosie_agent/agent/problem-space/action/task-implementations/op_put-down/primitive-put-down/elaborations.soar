sp {primitive-put-down*external*elaborate*problem-space*attend-to-all-perception
   (state <s> ^name primitive-put-down
              ^problem-space.execution-type external
              ^problem-space.perception <p>)
-->
   (<p> ^attend-to-all-perception yes)
}

sp {primitive-put-down*elaborate*held-object*task-operator
   (state <s> ^name primitive-put-down
              ^task-operator.arg1.id <obj>)
-->
   (<s> ^held-object <obj>)
}

sp {primitive-put-down*elaborate*object-info*perception-monitor
   (state <s> ^name primitive-put-down
              ^problem-space.execution-type external
              ^top-state.perception-monitor.object-monitor.object-info <obj-info>
              ^held-object <obj>)
   (<obj-info> ^wm-obj <obj>)
-->
   (<s> ^object-info <obj-info>)
}

sp {primitive-put-down*elaborate*predicate-handle*task-operator
   (state <s> ^name primitive-put-down
              ^task-operator.arg2.handle <pred-handle>)
-->
   (<s> ^predicate-handle <pred-handle>)
}

sp {primitive-put-down*elaborate*destination*task-operator
   (state <s> ^name primitive-put-down
              ^task-operator.arg2.{2} <obj>)
-->
   (<s> ^destination <obj>)
}

sp {primitive-put-down*elaborate*executing-command*true
   (state <s> ^name primitive-put-down
              ^current-task-segment.put-down-command.status << sent executing >>)
-->
   (<s> ^executing-command true)
}

sp {primitive-put-down*elaborate*propose-wait*true
   (state <s> ^name primitive-put-down
              ^executing-command true)
-->
   (<s> ^propose-wait true)
}
