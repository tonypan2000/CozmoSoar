sp {learn-subtask-policy*propose*task-operator
   (state <s> ^name learn-subtask-policy
              ^task-operator <o>)
-->
   (<s> ^operator <o> + >)
}

sp {learn-subtask-policy*elaborate*task-operator*with*created-subgoal
   (state <s> ^name learn-subtask-policy
              ^operator <o> +
              ^task-operator <o>
              ^created-procedural-subgoal <subgoal>)
-->
   (<o> ^current-subgoal <subgoal>)
}
