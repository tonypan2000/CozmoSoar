sp {subproblem*retrospective*task-operator*elaborate*problem-space
   (state <s> ^superstate.operator <o>
              ^superstate.task-operator <o>
              ^problem-space <ps>)
   (<ps> ^name action
         ^subproblem retrospective
         ^world <world>
         ^subtasks <subs>)
-->
   (<ps> ^execution-type internal)
   (<world> ^copy-type shallow)
   (<subs> ^copy-type shallow)
}

sp {learn-subtask-policy*task-operator*force*state-no-change*if*no*goal
   (state <s> ^superstate.name learn-subtask-policy
              ^superstate.operator.item-type task-operator
             -^desired
              ^operator <o> +)
-->
   (<s> ^operator <o> -)
}
