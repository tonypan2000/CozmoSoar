sp {push-procedural-subgoal*propose*report-success
   (state <s> ^name push-procedural-subgoal
              ^stored-subgoal true)
-->
   (<s> ^operator <o> +)
   (<o> ^name report-success)
}

sp {push-procedural-subgoal*apply*report-success
   (state <s> ^name push-procedural-subgoal
              ^operator.name report-success
              ^added-subgoal.handle <h>
              ^superstate <ss>)
-->
   (<ss> ^created-procedural-subgoal <h>)
}
