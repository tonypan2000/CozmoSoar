sp {push-procedural-subgoal*reject*force-operator-tie
   (state <s> ^name push-procedural-subgoal
              ^operator <o> +)
   (<o> ^name force-operator-tie)
-->
   (<s> ^operator <o> -)
}

sp {push-procedural-subgoal*prefer*retrievals*over*create-goal
   (state <s> ^name push-procedural-subgoal
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name << smem-query retrieve-subtask-handle >>)
   (<o2> ^name << create-goal-id add-subgoal-to-goal-id >>)
-->
   (<s> ^operator <o1> > <o2>)
}
