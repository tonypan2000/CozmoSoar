sp {learn-proposal-rule*task-operator*state-no-change*propose*report-failure
   (state <s> ^impasse no-change
              ^attribute state
              ^superstate.superstate.name learn-proposal-rule-substate)
-->
   (<s> ^operator <o> + ! =)
   (<o> ^name report-learn-proposal-rule-failure)
}

sp {learn-proposal-rule*task-operator*state-no-change*apply*report-failure
   (state <s> ^operator.name report-learn-proposal-rule-failure
              ^quiescence t
              ^superstate.superstate.superstate.superstate <sss>)
-->
   (<sss> ^learn-proposal-rule-failed true)
}
