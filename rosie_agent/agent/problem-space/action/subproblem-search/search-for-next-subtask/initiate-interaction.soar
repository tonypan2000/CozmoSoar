sp {search-for-next-subtask*propose*initiate-interaction*get-next-subaction
   (state <s> ^name search-for-next-subtask
              ^subtask-search-failed 1)
-->
   (<s> ^operator <o> + >)
   (<o> ^name initiate-interaction
        ^type get-next-subaction)
}

sp {search-for-next-subtask*prefer*initiate-interaction*over*return-search-failure
   (state <s> ^name search-for-next-subtask
              ^operator <o1> +
              ^operator <o2> +)
   (<o1> ^name initiate-interaction)
   (<o2> ^name return-search-failure)
-->
   (<s> ^operator <o1> > <o2>)
}

sp {search-for-next-subtask*apply*initiate-interaction*get-next-subaction*mark*policy-learning-needed
   (state <s> ^name search-for-next-subtask
              ^operator <o>
              ^quiescence t
              ^superstate.superstate.current-task-segment <seg>)
   (<o> ^name initiate-interaction
        ^type get-next-subaction)
-->
   (<seg> ^policy-learning-needed true)
}
