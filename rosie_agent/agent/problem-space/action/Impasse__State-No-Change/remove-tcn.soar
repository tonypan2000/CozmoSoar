sp {task-state-no-change*propose*remove-tcn
   (state <s> ^name task-state-no-change
              ^created-subtask <sub>)
-->
   (<s> ^operator <o> + =)
   (<o> ^name remove-tcn)
}

sp {task-state-no-change*apply*remove-tcn*remove*tcn
   (state <s> ^name task-state-no-change
              ^operator.name remove-tcn
              ^task-concept-network <tcn>)
-->
   (<s> ^task-concept-network <tcn> -)
}

sp {task-state-no-change*apply*remove-tcn*remove*created-sub
   (state <s> ^name task-state-no-change
              ^operator.name remove-tcn
              ^created-subtask <sub>)
-->
   (<s> ^created-subtask <sub> -)
}
