sp {task-utils*generalize-task-goal*elaborate*generalize-task-argument*predicate-set
   (state <s> ^name generalize-task-goal
              ^task-goal <goal>)
-->
   (<s> ^generalize-task-argument <gta>
        ^generalize-goal <gta>)
   (<gta> ^arg-type predicate-set
          ^instantiation <goal>)
}

