sp {task-utils*construct-task-operator*elaborate*world*from*top-state
  (state <s> ^name construct-task-operator
             ^top-state.world <world>)
-->
  (<s> ^world <world>)
}

# command-message: the parsed command representation from language comprehension
sp {task-utils*construct-task-operator*elaborate*message*from*superoperator
   (state <s> ^name construct-task-operator
              ^superstate.operator.command-message <msg>)
-->
   (<s> ^command-message <msg>)
}

# task-concept-network: The TCN for the task being constructed
sp {task-utils*construct-task-operator*elaborate*task-concept-network*from*superoperator
   (state <s> ^name construct-task-operator
              ^superstate.operator.task-concept-network <tcn>)
-->
   (<s> ^task-concept-network <tcn>)
}
