
sp {task-utils*push-task-segment*elaborate*smem-query*predicate
   (state <s> ^name push-task-segment
              ^segment-to-push.task-operator <task-op>
             -^predicate-lti.handle <pred-handle>)
   (<task-op> ^<arg-name> <arg>)
   (<arg> ^arg-type partial-predicate
          ^handle <pred-handle>)
-->
   (<s> ^smem-query <q>)
   (<q> ^cue.handle <pred-handle>
        ^depth 4
        ^destination <s>
        ^result-name predicate-lti)
}

