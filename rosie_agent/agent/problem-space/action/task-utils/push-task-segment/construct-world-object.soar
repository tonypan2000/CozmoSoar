sp {task-utils*push-task-segment*elaborate*create-object-rep*implicit*object*slot
   (state <s> ^name push-task-segment
              ^object-slots.implicit <obj-slot>)
-->
   (<s> ^create-object-rep <cor>)
   (<cor> ^source <obj-slot>)
}

# If the object is an smem object, create a world reprensetation for it
sp {task-utils*push-task-segment*propose*construct-world-object*create-object-rep
   (state <s> ^name push-task-segment
              ^create-object-rep <cor>)
   (<cor> ^source <src-obj>
         -^object-rep)
-->
   (<s> ^operator <o> + =)
   (<o> ^name construct-world-object
        ^source-type default-arg
        ^source-obj <src-obj>
        ^destination <cor>
        ^result-name object-rep)
}

