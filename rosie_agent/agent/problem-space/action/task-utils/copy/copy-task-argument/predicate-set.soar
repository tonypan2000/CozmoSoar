### COPY PREDICATE SET

sp {task-utils*copy-task-argument*expand*predicate-set*predicate*1
   (state <s> ^copy-task-argument <arg>)
   (<arg> ^arg-type predicate-set
          ^source-arg.{1} <pred1>
          ^object-map <map>)
-->
   (<arg> ^copy-pred1 <copy1>)
   (<copy1> ^arg-type predicate
            ^source-arg <pred1>
            ^object-map <map>)
   (<s> ^copy-task-argument <copy1>)
}

