
sp {learn-task-action-model*elaborate*instantiate-task-goal
   (state <s> ^name learn-task-action-model
              ^procedural-id <proc-id>
              ^task-operator <task-op>
              ^goal-id.predicates <gpreds>
              ^world <w>)
   (<gpreds> -^conditions)
-->
   (<s> ^instantiate-task-goal <ins-goal>)
   (<ins-goal> ^general-goal-rep <gpreds>
               ^reference-tcn-proc <proc-id>
               ^reference-task-op <task-op>
               ^world <w>)
}

sp {learn-task-action-model*elaborate*instantiate-task-goal*conditions
   (state <s> ^name learn-task-action-model
              ^procedural-id <proc-id>
              ^task-operator <task-op>
              ^instantiate-task-argument <arg>
              ^goal-id.predicates <gpreds>
              ^world <w>)
   (<arg> ^arg-type predicate-set
          ^type goal-conditions
          ^goal-predicates <gpreds>
          ^instantiation.satisfied true)
-->
   (<s> ^instantiate-task-goal <ins-goal>)
   (<ins-goal> ^general-goal-rep <gpreds>
               ^reference-tcn-proc <proc-id>
               ^reference-task-op <task-op>
               ^world <w>)
}

sp {learn-task-action-model*elaborate*desired*grounded
   (state <s> ^name learn-task-action-model
              ^instantiate-task-goal <ins-goal>
              ^goal-id <gid>)
   (<gid> ^handle <h>
          ^predicates <gpreds>)
   (<ins-goal> ^general-goal-rep <gpreds>
               ^instantiation <des>)
   (<gid> ^handle <h>)
-->
   (<s> ^created-desired <des>)
   (<des> ^handle <h>)
}

