sp {subproblem*execute*propose*init-current-subgoal
   (state <s> ^problem-space.subproblem execute
              ^name <task-name>
              ^task-operator.name <task-name>
              ^task-operator <task-op>
              ^task-concept-network.goal.{1}.handle <subgoal-handle>)
   (<task-op> -^current-subgoal)
-->
   (<s> ^operator <o> + > =)
   (<o> ^name init-current-subgoal
        ^subgoal-handle <subgoal-handle>)
}

sp {subproblem*execute*apply*init-current-subgoal
   (state <s> ^problem-space.subproblem execute
              ^operator <o>
              ^task-operator <task-op>)
   (<o> ^name init-current-subgoal
        ^subgoal-handle <subgoal-handle>)
-->
   (<task-op> ^current-subgoal <subgoal-handle>)
}


