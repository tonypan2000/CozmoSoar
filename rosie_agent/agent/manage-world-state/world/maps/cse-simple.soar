sp {topstate*elaborate*map
   (state <s> ^superstate nil)
-->
   (<s> ^maps <maps>)
   (<maps> ^map <building> <world>)

   (<world> ^handle world-map ^waypoint <bwp01>)
   (<bwp01> ^handle bwp01 ^x 0 ^y 0 ^map <world> ^sub-map <building>)

   ### BUILDING ###

   (<building> ^handle bmap1 ^super-waypoint <bwp01>
       ^waypoint <wp49> <wp51> )

   ### WAYPOINTS ###

  (<wp49> ^handle wp49 ^handle-int 49 ^x -41.00 ^y -34.50 ^map <building>)
   (<wp49> ^edge <e4951>)
    (<e4951> ^start <wp49> ^end <wp51> ^wall-side -1)

  (<wp51> ^handle wp51 ^handle-int 51 ^x -36.00 ^y -34.50 ^map <building>)
   (<wp51> ^edge <e5149>)
    (<e5149> ^start <wp51> ^end <wp49> ^wall-side 1)

}
