
sp {anystate*apply*attend-to-scene-change*print-perception*report-change*belief
   (state <s> ^operator <o>
              ^agent-params.print-perception true)
   (<o> ^name attend-to-scene-change
        ^change <c>)
   (<c> ^type <type>
        ^object-info.object-handle <handle>)
-->
   (write |Change | <type> | on object | <handle> (crlf))
}

sp {anystate*apply*attend-to-scene-change*print-perception*report-change*arm*actions
   (state <s> ^operator <o>
              ^agent-params.print-perception true)
   (<o> ^name attend-to-scene-change
        ^change <c>)
   (<c> ^type { <type> << arm-status-change >> })
-->
   (write |Change | <type> (crlf))
}

sp {anystate*apply*attend-to-scene-change*print-perception*new-object*print*message
    (state <s> ^agent-params.print-perception true
               ^operator <o>)
    (<o> ^name attend-to-scene-change ^change <chg>)
    (<chg> ^type new-object 
           ^<obj-type>.handle <h>)
-->
    (write |attend-to-scene-change [new-object]: | <h> (crlf))
}
